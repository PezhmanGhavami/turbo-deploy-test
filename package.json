{
  "name": "turbo-test",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build-web": "turbo build --filter=web",
    "build-docs": "turbo build --filter=docs",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "start": "turbo start",
    "start-web": "turbo start --filter=web",
    "start-docs": "turbo start --filter=docs",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
